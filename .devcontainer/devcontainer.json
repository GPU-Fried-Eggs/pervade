{
  "name": "pervade",
  "build": {
    "dockerfile": "./Dockerfile",
    "context": "."
  },
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": "true",
      "username": "pervade",
      "userUid": "3000",
      "userGid": "3000"
    },
    "ghcr.io/devcontainers/features/rust:1": {
      "version": "latest",
      "targets": [
        "wasm32-unknown-unknown",
        "wasm32-wasip1"
      ]
    },
    "ghcr.io/devcontainers/features/node:1": {
      "version": "lts"
    }
  },
  "remoteUser": "pervade"
}